@article{chinen2017vocationalbusinesstraining,
  title = {Vocational and Business Training to Improve Women's Labour Market Outcomes in Low- and Middle-Income Countries: A Systematic Review},
  author = {Chinen, M and T, De Hoop and Alc{\'a}zar, L and Balarin, M and Sennett, J},
  year = {2017},
  journal = {Campbell Systematic Reviews},
  volume = {16},
  publisher = {John Wiley \& Sons Ltd on behalf of The Campbell Collaboration.},
  abstract = {Vocational training has small positive effects on employment, formal employment, earnings and income Intervention: The review summarises evidence on the impacts of vocational and business training targeted at women in LMIC. Study questions: 1) What are the effects of vocational and business training programmes on women's income, working conditions, societal worth and economic empowerment? 2)What are the barriers to and facilitators of vocational and business training programme effectiveness? 3) What are the programme design and implementation characteristics associated with effective vocational and business training programmes for women? Inclusion criteria: The review included only experimental and quasi-experimental evaluations which measured the impact of vocational and business training programs in women 18 years and older, in low- and middle-income countries. The review also included qualitative and mixed-methods studies. The review also included studies published in English or Spanish and conducted after 1990 Number of included studies: The review included a total of 35 quantitative studies describing the impact of 30 interventions. The qualitative narrative meta-synthesis included findings on barriers to and facilitators of intervention effectiveness from 50 studies. Context/settings of included studies: Most of the included studies of vocational training took place in Latin America (n=11), with 3 from Africa, 2 from Asia and 2 from the Middle East. The business studies were distributed across the regions of Latin America (n=5), Africa (n=4) and Asia (n=3). The 5 studies examining the impact of the mixed interventions were all conducted in Africa in the countries of C{\^o}te d'Ivoire, Liberia, Malawi, Rwanda and Uganda. Study findings: Vocational training, on average, had positive but small effects on the employment, formal employment, earnings and income of women in LMIC. On average, vocational training increased the likelihood of employment and formal employment by 11\% (95\% CI=1.03, 1.18, evidence from 8 programmes) and 8\% (95\% CI=1.00, 1.18; evidence from 5 programmes) respectively. The average effect of vocational training on earnings was 5.54\% (95\% Ci=2.5\%, 8.96\%; evidence from 8 programs) or 0.11 standardised mean differences (95\% CI=0.05, 0.18; evidence from 8 programmes). There was no evidence that vocational training programmes that did and did not include life studies training or internships had different effects on employment, though there was some evidence that the effects of vocational training on earnings may increase with the inclusion of life skills training or an internship. Business training combined with cash transfers or life skills training had positive and statistically significant effects on the likelihood of self-employment. On average, business training increased women's sales or profits by 0.10 standardised mean differences (95\% CI=0.00, 0.20; evidence from four programmes).},
  file = {/Users/dtay0016/Zotero/storage/SMV77BM6/Chinen et al. - 2017 - Vocational and business training to improve women'.pdf}
}

